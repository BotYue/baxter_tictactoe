<!-- Launching all required nodes for playing Tic Tac Toe using smooth movements of Baxter's arms -->
<launch>

    <param name="cheating" type="bool" value="true" /> <!-- the robot cheats -->

    <param name="smooth" type="bool" value="false" /> <!-- mechanistic-robotic movements -->

    <include file="$(find tictactoe)/launchers/tictactoe.launch" />

</launch>
